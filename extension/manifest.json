{
  "name": "Twitter Sentiments",
  "description": "Extension to detect tweet sentiments",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hi.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://twitter.com/*", "https://mobile.twitter.com/*"]
    }
  ]
}
